{
  "name": "cybersecurity-chatbot",
  "version": "1.0.0",
  "description": "An AI chatbot focused on cybersecurity awareness.",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "prod": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop cybersecurity-chatbot",
    "pm2:restart": "pm2 restart cybersecurity-chatbot",
    "pm2:logs": "pm2 logs cybersecurity-chatbot",
    "pm2:delete": "pm2 delete cybersecurity-chatbot",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.0",
    "@types/node": "^16.0.0",
    "jest": "^27.0.0",
    "ts-jest": "^27.0.0",
    "ts-node": "^10.9.2"
  },
  "keywords": [
    "AI",
    "chatbot",
    "cybersecurity",
    "awareness"
  ],
  "author": "Your Name",
  "license": "MIT"
}
